{
  "bindings": [
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      // every night at 2 AM
      "schedule": "0 0 * * * *"
    },
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get","post"],
      "route": "ingest-notion"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
